  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@emotion/react": "^11.4.0",
    "babel-preset-gatsby-package": "^1.0.0",
    "cross-env": "^7.0.3"
  }
